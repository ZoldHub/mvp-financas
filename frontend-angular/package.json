{
  "name": "frontend-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:frontend-angular": "node dist/frontend-angular/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "~20.3.13",
    "@angular/compiler": "~20.3.13",
    "@angular/core": "~20.3.13",
    "@angular/forms": "~20.3.13",
    "@angular/platform-browser": "~20.3.13",
    "@angular/platform-server": "~20.3.13",
    "@angular/router": "~20.3.13",
    "@angular/ssr": "~20.3.13",
    "@ngneat/tailwind": "^7.0.3",
    "express": "^5.1.0",
    "ng-zorro-antd": "^20.4.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~20.3.0",
    "@angular/cli": "~20.3.13",
    "@angular/compiler-cli": "~20.3.13",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "autoprefixer": "^10.4.22",
    "jsdom": "^27.1.0",
    "less": "^4.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
